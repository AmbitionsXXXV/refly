{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:schema": "prisma format",
    "sync-db-schema": "node -r ts-node/register --env-file=.env scripts/sync-db-schema.ts",
    "proto:generate": "protoc -I ../protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=nestJs=true --ts_proto_out=./src/protos ../protos/aero.proto",
    "migrate:dev": "prisma migrate dev --create-only",
    "migrate:deploy": "prisma migrate deploy",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "postinstall": "prisma generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@dqbd/tiktoken": "1.0.7",
    "@langchain/community": "^0.0.32",
    "@langchain/openai": "^0.0.14",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "~2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "~10.0.3",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/passport": "~9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.50.0",
    "@opentelemetry/sdk-node": "^0.50.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "5",
    "@prisma/instrumentation": "^5.12.1",
    "avsc": "^5.7.7",
    "bull": "^4.12.2",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "~1.4.6",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "langchain": "^0.1.21",
    "lodash.omit": "^4.5.0",
    "lru-cache": "^10.2.0",
    "minio": "^7.1.3",
    "nestjs-pino": "^4.0.0",
    "node-fetch": "^2.0.0",
    "node-html-parser": "^6.1.13",
    "passport": "~0.6.0",
    "passport-github2": "~0.1.12",
    "passport-google-oauth20": "~2.0.0",
    "passport-jwt": "~4.0.1",
    "passport-local": "~1.0.0",
    "pg": "^8.11.5",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "weaviate-ts-client": "^2.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "~1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "18.16.12",
    "@types/passport": "~1.0.12",
    "@types/passport-github2": "~1.2.5",
    "@types/passport-google-oauth20": "~2.0.11",
    "@types/passport-jwt": "~3.0.8",
    "@types/passport-local": "~1.0.35",
    "@types/pg": "^8.11.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.172.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "rootDir": "src",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "testTimeout": 40000,
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

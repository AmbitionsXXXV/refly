{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint:ci": {},
    "lint": {},
    "clean": {
      "cache": false,
      "outputs": ["packages/*/build"]
    }
  }
}
